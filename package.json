      "/config",
      "/dist/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/setupJest.ts"
  },
  "greenkeeper": {
    "ignore": [
      "@angular/core",
      "@angular/common",
      "@angular/forms",
      "@angular/compiler",
