{
  "name": "@angular-material-extensions/google-maps-autocomplete-demo",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Demo app for @angular-material-extensions/google-maps-autocomplete",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4021",
    "serve:prod": "ng serve --prod",
    "build": "ng build",
    "build:doc": "cd ../ && gulp build:doc",
    "build:prod": "ng build --prod ",
    "build:client-and-server-bundles": "ng build --prod &&  ng run ngx-linkifyjs-demo:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "compile:server": "tsc -p server.tsconfig.json",
    "e2e": "ng e2e",
    "generate:prerender": "cd dist && node prerender",
    "lint": "ng lint",
    "test": "ng test",
    "postinstall": "npm run link",
    "link": "npm link @angular-material-extensions/google-maps-autocomplete",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@angular-material-extensions/faq": "^3.0.4",
    "@angular-material-extensions/jumbotron": "^1.0.1",
